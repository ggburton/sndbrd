{
    "name": "sndbrd",
    "version": "1.0.0",
    "description": "",
    "main": "src/app/main.js",
    "scripts": {
        "start": "electron src/app/main.js",
        "pack": "build --dir",
        "dist": "build",
        "postinstall": "install-app-deps"
    },
    "author": "",
    "license": "ISC",
    "build": {
        "appId": "com.example.soundboard",
        "productName": "soundboard",
        "win": {
            "target": [
