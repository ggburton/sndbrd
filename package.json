        },
        "nsis": {
            "installerIcon": "src/assets/settings/arcade.ico",
            "uninstallerIcon": "src/assets/settings/arcade.ico",
            "runAfterFinish": true
        }
    },
    "dependencies": {},
    "devDependencies": {
        "devtron": "^1.4.0",
        "electron": "^1.7.8",
        "electron-builder": "^19.34.1",
        "electron-devtools-installer": "^2.2.0",
        "electron-reload": "^1.2.1"
    }
}